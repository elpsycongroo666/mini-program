/* 楼层 S */
.floor .floor_group .floor_group_content {
  /* padding : 10rpx */
  /* 清除浮动 */
  border-left: 5rpx solid #fff;
  border-right: 5rpx solid #fff;
  overflow: hidden;
}
.floor .floor_group .floor_group_content navigator {
  float: left;
  width: 33.33%;
  border-left: 5rpx solid #fff;
  border-right: 5rpx solid #fff;
}
.floor .floor_group .floor_group_content navigator:nth-last-child(-n + 4) {
  /* 变化后的高度 = 原图的高度 * 750prx / 原图的宽度 */
  height: 207.97413793rpx;
}
.floor .floor_group .floor_group_content navigator:nth-last-child(-n + 4) image {
  height: 100%;
}
.floor .floor_group .floor_group_content navigator:nth-last-child(-n + 2) {
  border-top: 5rpx solid #fff;
}
